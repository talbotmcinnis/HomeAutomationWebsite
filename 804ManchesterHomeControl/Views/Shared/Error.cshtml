@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h2>
    Sorry, an error occurred while processing your request.  Call <del>tech support</del> Talbot.
</h2>

<h3>Details:</h3>
<p>Controller: @Model..ControllerName</p>
<p>Action: @Model.ActionName</p>
<p>Message: @Model.Exception.Message</p>
<p>Stack: @Model.Exception.StackTrace</p>



